@using PagedList.Mvc;
@using PagedList;
@using ALNWEB.Models;


@model PagedList.IPagedList<EO_ALANSIS_SATIS>

@{
    ViewBag.Title = "Onaylist";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

&nbsp;&nbsp;
<table id="onaylist" class="table table-striped table-bordered table-hover " width="100" cellspacing="0">

    <tr>
        <th>ID</th>
        <th>PARTI</th>
        <th>URETIM_ONAY</th>
    </tr>
    <tbody>
        @foreach (var eonay in Model)
        {
        <tr>
            <th>@eonay.ID</th>
            <th>@eonay.PARTI</th>
            <td>@eonay.URETIM_ONAY</td>

        </tr>
        }
    </tbody>
</table>
@Html.PagedListPager((IPagedList)Model, sayfa => Url.Action("Oaskilist", new { sayfa }));


